{
  "permissions": {
    "defaultMode": "acceptEdits",
    "allow": [
      "Bash(npx tsx *)",
      "Bash(npm run *)",
      "Bash(npm install*)",
      "Bash(npm test*)",
      "Bash(node *)",
      "Bash(git add *)",
      "Bash(git commit *)",
      "Bash(git status*)",
      "Bash(git log*)",
      "Bash(git diff*)",
      "Bash(git pull*)",
      "Bash(ls *)",
      "Bash(cat *)",
      "Bash(cp *)",
      "Bash(mkdir *)"
    ],
    "deny": [
      "Read(./.env)",
      "Read(./.env.*)"
    ]
  }
}
