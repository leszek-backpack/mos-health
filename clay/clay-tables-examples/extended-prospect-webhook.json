    {
      "index": 15,
      "name": "Step X | Check if Company Exists in HubSpot | HubSpot",
      "type": "action",
      "typeSettings": {
        "dataTypeSettings": {
          "type": "json"
        },
        "actionKey": "http-api-v2",
        "actionVersion": 1,
        "actionPackageId": "4299091f-3cd3-4d68-b198-0143575f471d",
        "inputsBinding": [
          {
            "name": "method",
            "formulaText": "\"POST\""
          },
          {
            "name": "url",
            "formulaText": "\"https://api.hubapi.com/crm/v3/objects/companies/search\""
          },
          {
            "name": "body",
            "formulaText": "\"{\\n  \\\"filterGroups\\\": [\\n    {\\n      \\\"filters\\\": [\\n        {\\n          \\\"propertyName\\\": \\\"domain\\\",\\n          \\\"operator\\\": \\\"EQ\\\",\\n          \\\"value\\\": \\\"\" + Clay.formatForJSON({{@Company Domain}}) + \"\\\"\\n        }\\n      ]\\n    }\\n  ],\\n  \\\"properties\\\": [\\n    \\\"name\\\",\\n    \\\"domain\\\",\\n    \\\"industry\\\",\\n    \\\"numberofemployees\\\",\\n    \\\"lifecyclestage\\\"\\n  ],\\n  \\\"limit\\\": 1\\n}\""
          },
          {
            "name": "headers",
            "formulaMap": {
              "Content-Type": "\"application/json\"",
              "Authorization": "\"Bearer \" + Clay.formatForJSON({{@Lookup Config}}?.[\"hubspot_token\"])"
            }
          },
          {
            "name": "removeNull",
            "formulaText": "true"
          },
          {
            "name": "followRedirects",
            "formulaText": "true"
          },
          {
            "name": "shouldRetry",
            "formulaText": "true"
          }
        ],
        "conditionalRunFormulaText": "!!{{@Company Domain}} && [\"1\",\"2\"].includes({{@Tier}})",
        "customRateLimitRules": {
          "timeWindow": [
            {
              "limit": 5,
              "bucket": [
                "WORKSPACE_ID"
              ],
              "durationMs": 1000
            }
          ]
        }
      }
    },
