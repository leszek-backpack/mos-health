    {
      "index": 9,
      "name": "LinkedIn Location Check",
      "type": "action",
      "typeSettings": {
        "dataTypeSettings": {
          "type": "json"
        },
        "actionKey": "use-ai",
        "actionVersion": 1,
        "actionPackageId": "67ba01e9-1898-4e7d-afe7-7ebe24819a57",
        "inputsBinding": [
          {
            "name": "useCase",
            "formulaText": "\"claygent\""
          },
          {
            "name": "prompt",
            "formulaText": "\"From this Li profile: \" + Clay.formatForAIPrompt({{@Prospect Li URL}}) + \", find:\\n\\n1) the profile's location\\n\""
          },
          {
            "name": "model",
            "formulaText": "\"gpt-4.1-nano\""
          },
          {
            "name": "answerSchemaType",
            "formulaMap": {
              "type": "\"json\"",
              "jsonType": "\"JSONSchema\"",
              "jsonSchema": "\"{\\n  \\\"type\\\": \\\"object\\\",\\n  \\\"properties\\\": {\\n    \\\"location\\\": {\\n      \\\"type\\\": \\\"string\\\",\\n      \\\"description\\\": \\\"The location listed on the LinkedIn profile\\\"\\n    }\\n  },\\n  \\\"required\\\": [\\n    \\\"location\\\"\\n  ],\\n  \\\"additionalProperties\\\": false\\n}\""
            }
          },
          {
            "name": "_metadata",
            "formulaMap": {
              "modelSource": "user"
            }
          }
        ],
        "conditionalRunFormulaText": "{{@Valid Profile?}} == \"Yes\"",
        "customRateLimitRules": {
          "timeWindow": [
            {
              "limit": 17,
              "bucket": [
                "PRIVATE_AUTH_KEY",
                "CUSTOM_KEY"
              ],
              "durationMs": 1000
            },
            {
              "limit": 750,
              "bucket": [
                "GLOBAL"
              ],
              "durationMs": 1000
            }
          ]
        },
        "authAccountId": "aa_0t3rf2hsVda3cBShEr6"
      }
    },
    {
      "index": 10,
      "name": "Location Classification",
      "type": "formula",
      "typeSettings": {
        "dataTypeSettings": {
          "type": "text",
          "label": "Text",
          "iconType": {
            "displayName": "TextT"
          }
        },
        "formulaType": "text",
        "formulaText": "{{@LinkedIn Location Check}}?.location?.toLowerCase()?.split(/[\\s,]+/)?.some(w => [\"california\",\"san\",\"francisco\",\"los\",\"angeles\",\"ca\"]?.includes(w)) ? \"California\" : \n{{@LinkedIn Location Check}}?.location?.toLowerCase()?.split(/[\\s,]+/)?.some(w => [\"new york\",\"ny\"]?.includes(w)) ? \"New York\" : \n{{@LinkedIn Location Check}}?.location?.toLowerCase()?.includes(\"united states\") || {{@LinkedIn Location Check}}?.location?.toLowerCase()?.split(/[\\s,]+/)?.some(w => [\"usa\",\"us\"]?.includes(w)) ? \"USA\" : \"\""
      }
    },
    {
      "index": 11,
      "name": "Role Category",
      "type": "formula",
      "typeSettings": {
        "dataTypeSettings": {
          "type": "text",
          "label": "Text",
          "iconType": {
            "displayName": "TextT"
          }
        },
        "formulaType": "text",
        "formulaText": "[\"human\",\"hr\",\"people\",\"office\",\"employee\",\"benefits\",\"chief of staff\",\"ceo\",\"founder\",\"coo\",\"admin\",\"workplace\",\"wellness\",\"facilities\",\"executive assistant\",\"happiness\",\"owner\"].some(k=>({{@Enrich Person}}?.current_title||\"\")?.toLowerCase()?.includes(k)) ? \"Qualified\" : \"DQ\""
      }
    },
    {
      "index": 12,
      "name": "Lead Score",
      "type": "action",
      "typeSettings": {
        "dataTypeSettings": {
          "type": "json"
        },
        "actionKey": "use-ai",
        "actionVersion": 1,
        "actionPackageId": "67ba01e9-1898-4e7d-afe7-7ebe24819a57",
        "inputsBinding": [
          {
            "name": "useCase",
            "formulaText": "\"use-ai\""
          },
          {
            "name": "prompt",
            "formulaText": "\"Score this prospect for B2B SaaS outreach relevance.\\n\\n**Prospect:**\\nName: \" + Clay.formatForAIPrompt({{@Full Name}}) + \"\\nTitle: \" + Clay.formatForAIPrompt({{@Enrich Person}}?.current_title) + \"\\nLocation: \" + Clay.formatForAIPrompt({{@LinkedIn Location Check}}?.location) + \"\\n\\n**Company:**\\nName: \" + Clay.formatForAIPrompt({{@Enrich Company}}?.name) + \"\\nEmployees: \" + Clay.formatForAIPrompt({{@Employee Count}}) + \"\\nDomain: \" + Clay.formatForAIPrompt({{@Company Domain}}) + \"\\n\\nScoring rules:\\n- Tier 1: Decision maker (VP+, C-level, Founder) at a company with 50-500 employees in the US\\n- Tier 2: Manager-level or relevant department head at qualifying company\\n- DQ: Outside US, company too small (<20) or too large (>1000), irrelevant role\""
          },
          {
            "name": "model",
            "formulaText": "\"gemini-2.5-flash\""
          },
          {
            "name": "answerSchemaType",
            "formulaMap": {
              "type": "\"json\"",
              "jsonType": "\"JSONSchema\"",
              "jsonSchema": "\"{\\n  \\\"type\\\": \\\"object\\\",\\n  \\\"properties\\\": {\\n    \\\"tier\\\": {\\n      \\\"type\\\": \\\"string\\\",\\n      \\\"description\\\": \\\"Qualification tier\\\",\\n      \\\"enum\\\": [\\n        \\\"1\\\",\\n        \\\"2\\\",\\n        \\\"DQ\\\"\\n      ]\\n    },\\n    \\\"reasoning\\\": {\\n      \\\"type\\\": \\\"string\\\",\\n      \\\"description\\\": \\\"One sentence explaining the scoring decision\\\"\\n    },\\n    \\\"buyingPersona\\\": {\\n      \\\"type\\\": \\\"string\\\",\\n      \\\"description\\\": \\\"Buying persona category\\\",\\n      \\\"enum\\\": [\\n        \\\"Founder\\\",\\n        \\\"C-Level\\\",\\n        \\\"VP/Director\\\",\\n        \\\"Manager\\\",\\n        \\\"Other\\\"\\n      ]\\n    }\\n  },\\n  \\\"required\\\": [\\n    \\\"tier\\\",\\n    \\\"reasoning\\\",\\n    \\\"buyingPersona\\\"\\n  ],\\n  \\\"additionalProperties\\\": false\\n}\""
            }
          },
          {
            "name": "_metadata",
            "formulaMap": {
              "modelSource": "user"
            }
          }
        ],
        "conditionalRunFormulaText": "{{@Location Classification}} && {{@Role Category}} == \"Qualified\"",
        "customRateLimitRules": {
          "timeWindow": [
            {
              "limit": 5,
              "bucket": [
                "PRIVATE_AUTH_KEY",
                "CUSTOM_KEY"
              ],
              "durationMs": 1000
            },
            {
              "limit": 750,
              "bucket": [
                "GLOBAL"
              ],
              "durationMs": 1000
            }
          ]
        },
        "authAccountId": "aa_0t6ag73mPirNqpgrotf"
      }
    },
