              "jsonSchema": "\"{\\n  \\\"type\\\": \\\"object\\\",\\n  \\\"properties\\\": {\\n    \\\"companyDescription\\\": {\\n      \\\"type\\\": \\\"string\\\",\\n      \\\"description\\\": \\\"1-2 sentence description of what the company does\\\"\\n    },\\n    \\\"workModel\\\": {\\n      \\\"type\\\": \\\"string\\\",\\n      \\\"description\\\": \\\"Observed work model\\\",\\n      \\\"enum\\\": [\\n        \\\"Hybrid\\\",\\n        \\\"In-Office\\\",\\n        \\\"Fully Remote\\\",\\n        \\\"Unknown\\\"\\n      ]\\n    },\\n    \\\"recentNews\\\": {\\n      \\\"type\\\": [\\n        \\\"string\\\",\\n        \\\"null\\\"\\n      ],\\n      \\\"description\\\": \\\"Recent news or funding, or null if nothing found\\\"\\n    },\\n    \\\"officePerks\\\": {\\n      \\\"type\\\": \\\"array\\\",\\n      \\\"description\\\": \\\"Physical office perks detected\\\",\\n      \\\"items\\\": {\\n        \\\"type\\\": \\\"string\\\"\\n      }\\n    }\\n  },\\n  \\\"required\\\": [\\n    \\\"companyDescription\\\",\\n    \\\"workModel\\\",\\n    \\\"recentNews\\\",\\n    \\\"officePerks\\\"\\n  ],\\n  \\\"additionalProperties\\\": false\\n}\""
